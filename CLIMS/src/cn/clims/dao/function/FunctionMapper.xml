<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.clims.dao.function.FunctionMapper">
	
	<select id="getFunctionListByRoleId" parameterType="Integer" resultType="function">
		select * from clims_function where roleId = #{roleId} or roleId=999
	</select>
	
</mapper>